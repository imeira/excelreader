{
  "name": "excel-reader",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm:start:app\" \"npm:start:mock\"",
    "start:app": "ng serve",
    "start:mock": "json-server --watch mock/data.json",
    "build": "ng build --prod",
    "test": "jest --no-cache",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:ci": "jest --no-cache --ci",
    "tdd": "jest --no-cache --watchAll --coverage false",
    "docs": "compodoc -d dist/docs -p src/tsconfig.app.json",
    "commit": "npx git-cz",
    "lint": "ng lint"
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": ">=6.0.0"
  },
  "private": true,
  "dependencies": {
   "@angular/animations": "^10.2.4",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/elements": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@quickweb/logger": "^5.0.1",
    "@types/chart.js": "^2.9.30",
    "@voxel/internet": "^4.0.0",
    "angular-bootstrap-md": "^10.1.1",
    "animate.css": "^4.1.1",
    "chart.js": "^2.5.0",
    "core-js": "^2.5.4",
    "document-register-element": "^1.13.3",
    "hammerjs": "^2.0.8",
    "ngx-csv-parser": "0.0.6",
    "rxjs": "^6.5.1",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
     "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@briebug/jest-schematic": "^1.5.1",
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "commitizen": "^3.1.1",
    "concurrently": "^4.1.0",
    "cz-conventional-changelog": "~2.1.0",
    "husky": "^1.3.1",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "json-server": "^0.16.1",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
